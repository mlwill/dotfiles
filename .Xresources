!! ~/.Xresources

URxvt*font:			xft:DejaVu Sans Mono:autohint=true:pixelsize=15
URxvt*boldFont:			xft:DejaVu Sans Mono:autohint=true:bold:pixelsize=15
URxvt*termName:			screen-256color
URxvt*geometry:  		123x40
URxvt*scrollBar:		false
URxvt*saveLines:		2000

!! Xft settings 
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

!! Disable Perl extensions 
URxvt.perl-ext:
URxvt.perl-ext-common:

!! Colors
URxvt*background:  #090C19
URxvt*foreground:  #E6E6E6
URxvt*cursorColor: #1AAA13 

! black
URxvt*color0:  #222946
! dark grey
URxvt*color8:  #8C8B89
 
! red
URxvt*color1:  #B32323
! bright red
URxvt*color9:  #DC2C2C
 
! green
URxvt*color2:  #1AAA13
! bright green
URxvt*color10: #5DCE33
 
! yellow
URxvt*color3:  #FF9F3E
! bright yellow
URxvt*color11: #E6CB29
 
! blue
URxvt*color4:  #4957F3
! bright blue
URxvt*color12: #4D79FF
 
! magenta
URxvt*color5:  #BF3E94
! bright magenta
URxvt*color13: #B641CC
 
! cyan
URxvt*color6:  #09A5B3
! bright cyan
URxvt*color14: #3CB6F3
 
! light grey
URxvt*color7:  #AEAEAE
! white
URxvt*color15: #DBDDE4

